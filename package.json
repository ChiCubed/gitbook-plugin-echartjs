{
  "name": "gitbook-plugin-echartjs",
  "version": "0.0.1",
  "description": "An extended Chart.js plugin for GitBook.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChiCubed/gitbook-plugin-echartjs.git"
  },
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "__dependencies__": {
    "chart.js": "^2.6.0",
    "chartjs-plugin-deferred": "^0.3.0"
  },
  "dependencies": {
    "chart.js": "^2.5.0",
    "chartjs-plugin-deferred": "^0.2.0",
    "copyfiles": "^1.0.0",
    "expr-eval": "^1.0.0"
  },
  "scripts": {
    "prepublish": "copyfiles -f node_modules/chart*/dist/*.js assets/"
  },
  "keywords": [
    "gitbook",
    "chart",
    "graph"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ChiCubed/gitbook-plugin-echartjs/issues"
  },
  "homepage": "https://github.com/ChiCubed/gitbook-plugin-echartjs#readme",
  "gitbook": {
    "properties": {
      "renderPNG": {
        "type": "boolean",
        "title": "Create PNG images from charts",
        "default": false
      }
    }
  }
}
